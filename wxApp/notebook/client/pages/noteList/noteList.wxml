<!-- pages/noteList/noteList.wxml -->
<view class="note-list">
  <view class="lists">
    <view class="list" wx:for="{{noteList}}" wx:key="index" bindtap="goNoteDetail" data-id="{{item.id}}">
      <view class="img">
        <image src="{{item.head_img}}" />
      </view>
      <view class="time">{{item.c_time}}</view>
      <view class="title">{{item.title}}</view>
    </view>
  </view>
</view>